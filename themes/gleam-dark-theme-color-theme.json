{
	"$schema": "vscode://schemas/color-theme",
	"name": "Gleam Dark",
	"type": "dark",
	"colors": {
	  // =========
	  // UI Geral
	  // =========
	  "focusBorder": "#f3b2ef",
	  "foreground": "#E0DEF4",
	  "descriptionForeground": "#948e93",
	  "errorForeground": "#ea6e92",
	  
	  // =========
	  // Editor
	  // =========
	  "editor.background": "#1e1e1e",
	  "editor.foreground": "#ffffff",
	  "editor.lineHighlightBackground": "#333333",
	  "editorLineNumber.foreground": "#948e93",
	  "editorLineNumber.activeForeground": "#E0DEF4",
	  "editorCursor.foreground": "#ffffff",
	  "editor.selectionBackground": "#363636",
	  "editor.inactiveSelectionBackground": "#36363680",
	  "editor.selectionHighlightBackground": "#9bced61a", 
	  "editor.selectionHighlightBorder": "#00000000",
	  "editor.wordHighlightBackground": "#59557166",      
	  "editor.findMatchBackground": "#363636",
	  "editor.findMatchHighlightBackground": "#36363680",
	  
	  // =========
	  // Barras e pain√©is
	  // =========
	  "sideBar.background": "#1e1e1e",
	  "sideBar.foreground": "#E0DEF4",
	  "sideBar.border": "#363636",
	  "activityBar.background": "#1e1e1e",
	  "activityBar.foreground": "#E0DEF4",
	  "activityBar.border": "#363636",
	  "activityBarBadge.background": "#E0DEF4",
	  "activityBarBadge.foreground": "#1e1e1e",
	  "statusBar.background": "#1e1e1e",
	  "statusBar.foreground": "#E0DEF4",
	  "statusBar.border": "#363636",
	  "panel.background": "#1e1e1e",
	  "panel.border": "#363636",
	  "editorGroup.border": "#363636",
	  "editorGroupHeader.tabsBackground": "#1e1e1e",
	  
	  // =========
	  // Tabs
	  // =========
	  "tab.activeBackground": "#1e1e1e",
	  "tab.activeForeground": "#ffffff",
	  "tab.inactiveBackground": "#151515",
	  "tab.inactiveForeground": "#948e93",
	  "tab.border": "#363636",
	  "tab.hoverBackground": "#363636",
	  
	  // =========
	  // Scrollbars
	  // =========
	  "scrollbarSlider.background": "#69696950",
	  "scrollbarSlider.hoverBackground": "#69696950",
	  "scrollbarSlider.activeBackground": "#69696950",
	  
	  // =========
	  // Terminal (ANSI)
	  // =========
	  "terminal.foreground": "#e0def4",
	  "terminal.background": "#1e1e1e",
	  "terminalCursor.foreground": "#ffffff",
	  "terminal.ansiBlack": "#1e1e1e",
	  "terminal.ansiBrightBlack": "#3f3b58",
	  "terminal.ansiRed": "#ea6e92",
	  "terminal.ansiBrightRed": "#7e3647",
	  "terminal.ansiGreen": "#5cc1a3",
	  "terminal.ansiBrightGreen": "#31614f",
	  "terminal.ansiYellow": "#f5c177",
	  "terminal.ansiBrightYellow": "#8a643a",
	  "terminal.ansiBlue": "#9bced6",
	  "terminal.ansiBrightBlue": "#566b70",
	  "terminal.ansiMagenta": "#a683a0",
	  "terminal.ansiBrightMagenta": "#51414e",
	  "terminal.ansiCyan": "#3e8fb0",
	  "terminal.ansiBrightCyan": "#264654",
	  "terminal.ansiWhite": "#e0def4",
	  "terminal.ansiBrightWhite": "#e0def4"
	},
	"tokenColors": [
	  // =======================
	  //  Sintaxe (tokenColors)
	  // =======================
	  {
		"name": "Comment",
		"scope": ["comment", "comment.line", "comment.block"],
		"settings": {
		  "foreground": "#888888",
		  "fontStyle": "italic"
		}
	  },
	  {
		"name": "Keyword & Boolean",
		"scope": [
		  "keyword", 
		  "storage.type", 
		  "storage.modifier",
		  "constant.boolean"
		],
		"settings": {
		  "foreground": "#FFD596"
		}
	  },
	  {
		"name": "Strings",
		"scope": [
		  "string",
		  "string.quoted",
		  "string.interpolated",
		  "string.template",
		  "string.escape",
		  "string.regex"
		],
		"settings": {
		  "foreground": "#C8FFA7"
		}
	  },
	  {
		"name": "Numbers",
		"scope": ["constant.numeric"],
		"settings": {
		  "foreground": "#fdffab"
		}
	  },
	  {
		"name": "Functions / Methods",
		"scope": [
		  "entity.name.function", 
		  "meta.method.js",
		  "support.function"
		],
		"settings": {
		  "foreground": "#9ce7ff"
		}
	  },
	  {
		"name": "Types (classes, enums, etc.)",
		"scope": [
		  "entity.name.type",
		  "support.class",
		  "support.type",
		  "entity.name.enum"
		],
		"settings": {
		  "foreground": "#ffddfa",
		  "fontStyle": "italic"
		}
	  },
	  {
		"name": "Constructor / Enum Variant",
		"scope": [
		  "variable.language.this", 
		  "entity.name.type.class", 
		  "support.constant"
		],
		"settings": {
		  "foreground": "#ffddfa",
		  "fontStyle": "italic"
		}
	  },
	  {
		"name": "Punctuation / Operators",
		"scope": [
		  "punctuation", 
		  "keyword.operator",
		  "punctuation.definition.tag",
		  "punctuation.definition.string",
		  "punctuation.separator"
		],
		"settings": {
		  "foreground": "#FFAFF3"
		}
	  },
	  {
		"name": "Tag",
		"scope": ["entity.name.tag"],
		"settings": {
		  "foreground": "#FE7AB2"
		}
	  },
	  {
		"name": "Attribute name in tags",
		"scope": [
		  "entity.other.attribute-name",
		  "meta.attribute",
		  "markup.bold.string"
		],
		"settings": {
		  "foreground": "#FFD596",
		  "fontStyle": "italic"
		}
	  },
	  {
		"name": "Property",
		"scope": ["variable.other.property", "support.type.property-name"],
		"settings": {
		  "foreground": "#9ce7ff"
		}
	  },
	  {
		"name": "Title / Headings",
		"scope": ["markup.heading", "entity.name.section"],
		"settings": {
		  "foreground": "#FFD596",
		  "fontStyle": "bold"
		}
	  },
	  {
		"name": "Variables",
		"scope": ["variable", "identifier"],
		"settings": {
		  "foreground": "#ffffff"
		}
	  }
	]
  }
  